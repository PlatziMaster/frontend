.interest-card__icon--container
    width: 100%
    height: auto
    display: grid
    grid-template-columns: repeat(5, 1fr)
    grid-template-rows: 60px
    justify-items: center
    align-items: center


.music-icon
    grid-column: 0 / 1
    grid-row: 1 / 1
    font-size: InterestIconSize

.sports-icon
    grid-column:1 / 2
    grid-row: 1 / 1
    font-size: InterestIconSize

.travel-icon
    grid-column: 2 / 3
    grid-row: 1 / 1
    font-size: InterestIconSize

.exercise-icon
    grid-column: 3 / 4
    grid-row: 1 / 1
    font-size: InterestIconSize

.picture-icon
    grid-column: 4 / 5
    grid-row: 1 / 1
    font-size: InterestIconSize
