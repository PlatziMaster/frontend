.Academic {
  margin-bottom 2.5rem
  &__list {
    padding-left: 1rem;
    margin 0
  }
  &-item {
    list-style square
    &__career {
      font-size: 0.95rem;
      font-weight: 700;
      letter-spacing: 1.5px;
      margin-bottom 0.5rem
      text-transform: uppercase;
    }
    &__info {
      font-size 0.9rem
      font-weight 400
      line-height 1.5rem
      margin 0
    }
  }
}