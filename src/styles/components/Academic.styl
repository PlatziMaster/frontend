.Academic-container
    padding: 5%
    background-color #2bb3dd
    .Academic-title
        font-size: 18px
        font-weight: 500
        text-align center
    .Academic-item
        margin 5% 3%
        .degr,
        .inst
            font-weight 500
            color #121A3E
        .desc,
        .enddate,
        .startdate
            font-weight 100
        .startdate
            padding 5% 2%
            font-size: 13px
        .enddate
            background-color #FED700
            padding 2px 5px 
            margin 0 1%
            border-radius 10px
            font-size: 12px
