html
    font-family 'Source Sans Pro', sans-serif
    font-size 62.5%

body
    //background-color #C0C0C0
    font-size 1.2rem

.About-list
    align-items center
    justify-content space-around
    list-style none
    display flex

.Header
    align-items center
    flex-wrap wrap
    display flex

.Header-text
    font-size 1.4rem
    margin-left 30px

.Header-photo
    height 8%
    width 8%
    background-repeat no-repeat
    background-position 50%
    border-radius 50%
    background-size 100% auto
    object-fit cover

.Card 
    box-shadow 0 8px 16px 0 rgba(0,0,0,0.2), 0 6px 20px 0 rgba(0,0,0,0.19)
    padding 20px

.Card-up
    box-shadow 0 8px 16px 0 rgba(0,0,0,0.2), 0 6px 20px 0 rgba(0,0,0,0.19)
    padding 20px
    background silver


.Percentage-list li
    display flex
    justify-content space-between
    list-style none

.Percentage-list
    padding-inline-start 0

.Interest-list
    list-style none
    padding 0

.Experience-item h2
    margin-block-end 0

.Experience-item h3, p 
    margin-block-start 0
    margin-block-end 0

.Academic-container h2
    margin-block-end 0

.Academic-container h3, p 
    margin-block-start 0
    margin-block-end 0

.columns-2
    display flex
