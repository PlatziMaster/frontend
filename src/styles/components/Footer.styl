.Footer-container
  display grid
  place-content center
  grid-template-columns: auto auto auto auto
  grid-gap: 1rem
  grid-area footer
  background-color lightness(#ff4500, 70%)
  border-radius 0 0 1rem 1rem
  margin 0
  padding 2rem
  color black
  align-content center
  .Social-item
    color black
    text-decoration none
    font-size 1.4rem
    &:visited
      color black
      text-decoration none