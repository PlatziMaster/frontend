
header
  background-color red
  display: flex
  flex-flow: row nowrap
  margin: 0 auto
  height: 50%
  align-items center
  flex-direction row
  padding-left 5%
  margin-right 10%

.HeaderItem
  flex-direction column
  margin-left 10%

.HeaderItemRow
  flex-direction column
  margin 5px
  margin-left 0

.fotoCV 
  width 200px
  border-collapse separate
  border-radius 100px

img:not(.fotoCV)
  width 15px
  
header img:not(:first-child) 
    color red
    padding-left 10px



