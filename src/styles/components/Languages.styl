.Languages-container
  grid-area languages
  background-color lightness(#ff4500, 80%)
  border-radius 1rem
  margin 0 2rem 2rem 2rem
  padding 2rem
  .Languages-items-container
    display grid
    grid-template-columns: 1fr 1fr
    grid-gap 1rem
    .Languages-name
      font-size 1.4rem
      font-weight 500
    .Languages-percentage
      font-size 1.4rem