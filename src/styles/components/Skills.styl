.Skills
  box-shadow 0 1px 3px 1px rgba(0,0,0,.4)
  background-color #E3DFC9
  margin 10px
  padding 4px

  ul
    list-style-type none

    li
      .Skills--Percentage
        display flex
        justify-content space-between
        margin 10px 0
        
        p
          flex 1
        .progress
          flex 2
          box-shadow 1px 1px 1px 1px #cfcfcf
          .progress--inner
            height 17px
            background-color #DEB88C
