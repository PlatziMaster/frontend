.Skills-title
    font-size 3rem
    text-transform uppercase
    letter-spacing 0.3rem

.skills__container
    margin: 30px 20px;
    flex-basis: 50%;

.skills__listContainer
    text-align: center;

.Skills-item{
    color:#000
    background-color:#f1f1f1
    border-radius:16px
    margin: 3px 20px 10px;
    text-align: center
}

.skills__progressbar{
    padding:0 10px
    color:#fff
    background-color:#689F38
    border-radius:16px
    text-align: left
}